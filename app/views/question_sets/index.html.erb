<p id="notice"><%= notice %></p>

<h1>Listing Question Sets</h1>
<ul>
<% @question_sets.each do |question_set| %>
    <li>
    <%= question_set[:name] %>
    <%= link_to 'Show', question_set %>
    <%= link_to 'Edit', edit_question_set_path(question_set) %>
    <%= link_to 'Destroy', question_set, method: :delete, data: { confirm: 'Are you sure?' } %>
    </li>
<% end %>
</ul>
<br>

<%= link_to 'New Question set', new_question_set_path %>
