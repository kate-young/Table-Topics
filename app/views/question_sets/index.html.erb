<h2 class="page-title">Listing Question Sets</h2>
<ul class="list-group">
<% @question_sets.each do |question_set| %>
    <li id="<%= dom_id(question_set) %>" class= "list-group-item">
    <%= question_set[:name] %>
    <%= link_to 'Show', question_set, class: "btn btn-link btn-xs pull-right" %>
    <%= link_to 'Edit', edit_question_set_path(question_set), class: "btn btn-link btn-xs pull-right" %>
    <%= link_to 'Destroy', question_set, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-link btn-xs pull-right" %>
    </li>
<% end %>
</ul>
<br>

<%= link_to 'Add New', new_question_set_path, class: "btn btn-default" %>
