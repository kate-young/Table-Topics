<p id="notice"><%= notice %></p>
<h1><%= @question_set[:name] %></h1>
<p><%= @question_set[:description] %></p>
<% if @question_set.questions.length > 0 %>
    <ul>
    <% @question_set.questions.each do |question| %>
        <li id="<%= dom_id(question) %>"><%= question[:value] %></li>
    <% end %>
    </ul>
<% end %>
<%= link_to 'Add Question', new_question_set_question_path(@question_set) %>
<%= link_to 'Edit', edit_question_set_path(@question_set) %> |
<%= link_to 'Back', question_sets_path %>
